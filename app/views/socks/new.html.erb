<%= simple_form_for(@sock, method: :post, url: socks_path) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :color %>
  <%= f.input :price %>
  <%= f.input :size %>
  <%= f.input :user_id %>
  <%= f.submit %>
<% end %>

