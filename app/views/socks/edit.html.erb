<%= simple_form_for(@sock, method: :patch, url: sock_path(@sock.id)) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :category %>
  <%= f.input :color %>
  <%= f.input :price %>
  <%= f.input :size %>
  <%= f.input :user_id %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit %>
<% end %>

